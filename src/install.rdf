<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>sdbtool@tottokug.com</em:id>
    <em:type>2</em:type>
    <em:version>__VERSION__.__BUILD__</em:version>
    <em:file resource="urn:mozilla:extension:file:sdbizo.jar"/>
   
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.5</em:minVersion>
        <em:maxVersion>20.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>SDBTool</em:name>
    <em:description>Mozilla Firefox extension for interacting with Amazon Simple DB (SDB).</em:description>
    <em:creator>Donnie Flood at Bizo, Inc (http://www.bizo.com)</em:creator>
    <em:homepageURL>http://www.bizo.com</em:homepageURL>
    <em:updateURL>https://s3.amazonaws.com/com-bizo-public/tools/sdbizo/sdbizo.rdf</em:updateURL>
  </Description>

  <Description about="urn:mozilla:extension:file:sdbizo.jar"
                   em:package="content/sdbizo/">
    <em:locale>locale/sdbizo/en-US/</em:locale>
  </Description>
</RDF>
